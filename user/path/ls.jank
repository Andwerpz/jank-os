#include <iostream>;
#include <assert>;
#include <wait>;
#include <dirent>;

i32 main() {

    dirstream* dir = opendir(".");
    while(!dir->eof()) {
        dirent* dent = readdir(dir);
        if($void* dent == nullptr) {
            //error reading
            break;
        }
        cout << ($u8* @(dent->d_name)) << "\n";
    }
    closedir(dir);
    
    return 0;
}
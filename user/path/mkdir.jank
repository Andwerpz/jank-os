#include <iostream>;
#include <assert>;
#include <wait>;
#include <dirent>;

i32 main(u64 argc, u8** argv) {
    if(argc == 0x1) {
        cout << "Usage : \n";
        cout << "mkdir <filepath>\n";
        return 1;
    }

    if(sys_mkdir(argv[1], $mode_t 0o755)) {
        cout << "mkdir failed\n";
        return 1;
    }

    return 0;
}
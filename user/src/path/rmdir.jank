#include <iostream>;
#include <assert>;
#include <wait>;
#include <dirent>;

i32 main(u64 argc, u8** argv) {
    if(argc == 0x1) {
        cout << "Usage : \n";
        cout << "rmdir <filepath>\n";
        return 1;
    }

    if(sys_rmdir(argv[1])) {
        cout << "rmdir failed\n";
        return 1;
    }

    return 0;
}